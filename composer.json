{
    "name": "bus-factor/laminas-doctrine",
    "description": "Laminas doctrine package for easy doctrine integration",
    "type": "library",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "authors": [
        {
            "name": "Michael LeÃŸnau",
            "email": "michael.lessnau@gmail.com"
        }
    ],
    "readme": "README.md",
